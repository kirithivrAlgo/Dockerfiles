{
  "name": "algoshack-automation",
  "version": "1.2.1",
  "description": "WebdriverIO with Cucumber and JavaScript automation framework",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "test": "wdio wdio.conf.js",
    "all": "wdio wdio.conf.js && allure generate --clean --single-file --output allure-report",
    "html-report": "node generate-html-report.js",
    "report": "allure generate --clean --single-file --output allure-report",
    "reports": "npx allure generate allure-results --clean -o allure-report && allure open allure-report"
  },
  "engines": {
    "node": ">=18"
  },
  "author": "Algoshack",
  "license": "MIT",
  "dependencies": {
    "@cucumber/cucumber": "10.9.0",
    "@wdio/allure-reporter": "^8.30.1",
    "@wdio/cli": "^9.12.4",
    "@wdio/cucumber-framework": "^9.12.3",
    "@wdio/local-runner": "^8.30.1",
    "@wdio/spec-reporter": "^8.30.1",
    "allure-commandline": "^2.14.0",
    "axios": "^1.8.4",
    "base-64": "^1.0.0",
    "canvas": "^3.1.0",
    "child_process": "^1.0.2",
    "chromedriver": "^142.0.0",
    "clipboardy": "^4.0.0",
    "exceljs": "^4.4.0",
    "glob": "^11.0.1",
    "js-yaml": "^4.1.0",
    "jsonpath-plus": "^10.3.0",
    "log4js": "^6.9.1",
    "pdfjs-dist": "^2.14.305",
    "pixelmatch": "^7.1.0",
    "wdio-chromedriver-service": "^8.0.0",
    "wdio-cucumber-html-reporter": "^0.0.36",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "cucumber-html-reporter": "^7.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3",
    "wdio-cucumberjs-json-reporter": "^5.0.0",
    "wdio-video-reporter": "^6.1.1"
  }
}
